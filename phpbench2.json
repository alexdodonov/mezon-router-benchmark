{
	"runner.bootstrap": "vendor/autoload.php",
	"runner.php_disable_ini": true,
    "runner.php_config": {
        "memory_limit": "512M"
    },
    "runner.path": "Mezon/Benchmarks2",
    "runner.output_mode": "throughput",
    "report.outputs": {
        "console_example": {
            "renderer": "console",
            "table_style": "symfony-style-guide"
        },
        "html_example": {
            "title": "PHPBench Examples",
            "renderer": "xslt",
            "file": "example.html"
        }
    }
}
